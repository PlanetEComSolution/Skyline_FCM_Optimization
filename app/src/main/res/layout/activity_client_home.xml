<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">
    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/pullToRefresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F6FDFF"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/linerfortime"
            android:layout_width="match_parent"
            android:layout_height="80sp"
            android:background="#ADE3F0"

            android:orientation="horizontal"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">

            <ImageView
                android:id="@+id/merchantname"
                android:layout_width="70dp"
                android:layout_height="40sp"
                android:layout_gravity="left|center_vertical"
                android:src="@drawable/exhibitlogoa" />


            <TextView
                android:id="@+id/textView1rr"
                android:layout_width="match_parent"

                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"


                android:gravity="center"

                android:text="Welcome"
                android:textColor="@color/btn_text_sky_blue"
                android:textSize="@dimen/headersname" />


            <RelativeLayout
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/client_logo"
                    android:layout_width="70dp"
                    android:layout_height="40sp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="right|center_vertical"


                    />

                <TextView
                    android:id="@+id/tv_ver"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="bottom"

                    android:textColor="@color/btn_text_sky_blue"
                    android:textSize="14sp" />

            </RelativeLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#CFF5FF"
            android:orientation="vertical">

            <View
                android:layout_width="@dimen/view_width"
                android:layout_height="@dimen/view_width" />

            <LinearLayout
                android:id="@+id/ll_billable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical"

                android:visibility="visible">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"


                    android:orientation="horizontal">

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/ll_Notification"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white"
                        android:gravity="center"

                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">
                            <ImageView

                                android:layout_width="45dp"
                                android:layout_height="45dp"
                                android:clickable="false"
                                android:layout_gravity="center"
                                android:src="@drawable/notification_bell" />


                            <Button
                                android:id="@+id/count"
                                android:layout_width="20dp"
                                android:layout_height="20dp"

                                android:layout_gravity="center_vertical"
                                android:layout_marginLeft="-20dp"
                                android:background="@drawable/round_corner"
                                android:elevation="4dp"
                                android:focusable="false"
                                android:focusableInTouchMode="false"
                                android:layout_marginBottom="9dp"
                                android:text="1"
                                android:backgroundTint="@android:color/holo_red_light"
                                android:textColor="#fff"
                                android:textSize="13sp"
                                android:textStyle="normal"

                                />



                        </LinearLayout>

                        <TextView

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="3dp"
                            android:gravity="center"

                            android:text="Notifications"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />

                    <LinearLayout
                        android:id="@+id/ll_PowerBoard"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@android:color/white"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/slot"
                            android:layout_width="45dp"
                            android:layout_height="45dp"

                            android:clickable="false"

                            android:src="@drawable/power_board" />
                        <!-- android:text="Notifications"-->
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="3dp"


                            android:gravity="center"
                            android:text="Powerboard"

                            android:textSize="@dimen/textSize_homeActivity" />


                    </LinearLayout>

                    <View
                        android:layout_width="@dimen/view_width"
                        android:layout_height="@dimen/view_width" />
                </LinearLayout>

                <View
                    android:layout_width="@dimen/view_width"
                    android:layout_height="@dimen/view_width" />
            </LinearLayout>

            <!--new chnges-->


            <!---->
